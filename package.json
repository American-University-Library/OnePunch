{
    "name": "onepunch",
    "productName": "One Punch",
    "version": "0.9.0",
    "description": "A logger for patron interactions",
    "main": "src/main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "keywords": [],
    "author": "ASPApp",
    "license": "MIT",
    "dependencies": {
        "auto-launch": "^5.0.1",
        "electron-settings": "^3.1.3"
    },

    "build": {
        "appId": "com.electron.onepunch",
        "productName": "One Punch",
        "directories": {
            "buildResources": "src/build"
        },
        "icon": "src/build/icon.ico",
        "nsis": {
            "installerIcon": "src/images/owl_ico.ico",
            "uninstallerIcon": "src/images/owl_ico.ico"
        }
    },
    "devDependencies": {
        "electron": "~1.7.8",
        "electron-builder": "^19.37.2",
        "electron-packager": "^9.1.0"
    }
}
